{
  "name": "z-drop-editor",
  "private": false,
  "version": "0.0.12",
  "type": "module",
  "main": "./lib/z-drop-editor.umd.js",
  "module": "./lib/z-drop-editor.es.js",
  "style": "lib/main.css",
  "scripts": {
    "dev:lib": "vite --config vite.config.lib.ts",
    "build:lib": "vite build --config vite.config.lib.ts && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "type-check": "vue-tsc"
  },
  "license": "MIT",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/z-drop-editor.d.ts",
      "import": "./lib/z-drop-editor.es.js",
      "require": "./lib/z-drop-editor.umd.js"
    },
    "./style": "./lib/main.css"
  },
  "dependencies": {
    "@types/node": "^22.13.9",
    "d3": "^7.9.0",
    "d3-quadtree": "^3.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-quadtree": "^3.0.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.85.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-libcss": "^1.1.1",
    "vue-tsc": "^2.2.0"
  }
}
